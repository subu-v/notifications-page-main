nhtml {\r\n  font-size: 62.5%;\r\n}\r\n\r\nbody {\r\n  background-color: var(--clr-body-bg);\r\n  font-family: \"Plus Jakarta Sans\", sans-serif;\r\n  @include flex(initial, center, center, 0);\r\n}\r\n\r\n:root {\r\n  --clr-red: hsl(1, 90%, 64%);\r\n  --clr-blue: hsl(219, 85%, 26%);\r\n  --clr-white: hsl(0, 0%, 100%);\r\n  --clr-body-bg: hsl(210, 60%, 98%);\r\n  --clr-dark-blue: hsl(224, 21%, 14%);\r\n  --clr-dark-greyish-blue: hsl(219, 12%, 42%);\r\n  --clr-light-greyish-blue-1: hsl(211, 68%, 94%);\r\n  // --clr-nofication-bg: ;\r\n}\r\n","sass\\abstracts\\_mixins.scss":"@mixin flex($direction, $justify-content, $align-items, $gap) {\r\n  display: flex;\r\n  flex-direction: $direction;\r\n  justify-content: $justify-content;\r\n  align-items: $align-items;\r\n  gap: $gap;\r\n}\r\n","sass\\main.scss":"@use \"abstracts\" as *;\r\n@use \"base\" as *;\r\n\r\n.notification {\r\n  padding: 6.2rem;\r\n  &__container {\r\n    background-color: var(--clr-white);\r\n    border-radius: 10px;\r\n\r\n    box-shadow: 0 0 10px 10px rgba(221, 231, 238, 0.25);\r\n    padding: 2.4rem 2.4rem 1.2rem 2.4rem;\r\n    width: 67rem;\r\n  }\r\n\r\n  &__header-box,\r\n  &__comment-box {\r\n    @include flex(row, space-between, center, 0);\r\n  }\r\n\r\n  &__header-box {\r\n    margin-bottom: 2.4rem;\r\n  }\r\n  &__primary-heading {\r\n    color: var(--clr-dark-blue);\r\n    font-size: 2.4rem;\r\n  }\r\n\r\n  &__count {\r\n    background-color: var(--clr-blue);\r\n    border-radius: 5px;\r\n    color: hsl(210, 60%, 98%);\r\n    font-size: 1.6rem;\r\n    font-weight: 800;\r\n    width: 3rem;\r\n    height: 2.4rem;\r\n    @include flex(initial, center, center, 0);\r\n  }\r\n\r\n  &__toggle {\r\n    color: var(--clr-dark-greyish-blue);\r\n    font-size: 1.6rem;\r\n    font-weight: 500;\r\n    &:hover {\r\n      color: var(--clr-blue);\r\n      cursor: pointer;\r\n    }\r\n  }\r\n\r\n  &__toggle,\r\n  &__profile-name,\r\n  &__subject {\r\n    transition: color 0.25s ease;\r\n  }\r\n\r\n  &__read-box,\r\n  &__unread-box,\r\n  &__comment,\r\n  &__header {\r\n    @include flex(row, initial, initial, 1.2rem);\r\n  }\r\n\r\n  &__header {\r\n    align-items: center;\r\n  }\r\n\r\n  &__unread-box,\r\n  &__read-box,\r\n  &__comment-box {\r\n    border-radius: 10px;\r\n    padding: 1.6rem;\r\n    margin-bottom: 1.2rem;\r\n  }\r\n\r\n  &__unread-box {\r\n    background-color: hsl(210, 60%, 98%);\r\n  }\r\n\r\n  &__profile-img,\r\n  &__commented-img {\r\n    width: 4.8rem;\r\n    height: 4.8rem;\r\n  }\r\n  &__desc,\r\n  &__private-msg {\r\n    color: var(--clr-dark-greyish-blue);\r\n    font-size: 1.6rem;\r\n    font-weight: 500;\r\n  }\r\n\r\n  &__desc {\r\n    margin-bottom: 0.4rem;\r\n  }\r\n\r\n  &__profile-name {\r\n    &:link,\r\n    &:visited {\r\n      color: var(--clr-dark-blue);\r\n      text-decoration: none;\r\n      font-weight: 800;\r\n    }\r\n    &:hover {\r\n      color: var(--clr-blue);\r\n    }\r\n  }\r\n\r\n  &__subject {\r\n    &:link,\r\n    &:visited {\r\n      font-weight: 800;\r\n      text-decoration: none;\r\n      color: hsl(219, 12%, 42%);\r\n    }\r\n\r\n    &:hover,\r\n    &:active {\r\n      color: var(--clr-blue);\r\n    }\r\n\r\n    &--chess {\r\n      &:link,\r\n      &:visited {\r\n        color: var(--clr-blue);\r\n      }\r\n    }\r\n  }\r\n\r\n  &__unread-mark {\r\n    display: inline-block;\r\n    width: 1rem;\r\n    height: 1rem;\r\n    background-color: var(--clr-red);\r\n    border-radius: 50%;\r\n  }\r\n\r\n  &__time-ago {\r\n    color: hsl(219, 14%, 63%);\r\n    font-size: 1.4rem;\r\n    font-weight: 500;\r\n\r\n    &--private-msg {\r\n      margin-bottom: 1.2rem;\r\n    }\r\n  }\r\n\r\n  &__private-msg {\r\n    padding: 1.8rem;\r\n    border-radius: 5px;\r\n    border: 2px solid var(--clr-light-greyish-blue-1);\r\n    transition: background-color 0.25s ease;\r\n\r\n    &:hover {\r\n      background-color: var(--clr-light-greyish-blue-1);\r\n    }\r\n  }\r\n\r\n  &__commented-img {\r\n    border: 3px solid transparent;\r\n    border-radius: 10px;\r\n    transition: border-color 0.25s ease;\r\n    cursor: pointer;\r\n    &:hover {\r\n      border-color: hsl(205, 33%, 90%);\r\n    }\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"f74c72a990d11fe0c574c695b088707c","cacheData":{"env":{}}}